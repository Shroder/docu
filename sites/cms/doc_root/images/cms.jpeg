		<script type="text/javascript">
							/*****  cookie functions  *****/
				jQuery.extend(ClickLib.Functions, {
					setCookie : function (name, value) {
						var argv = arguments;
		       			var argc = arguments.length;
						var expires = (argc > 2) ? argv[2] : null;
						var path = (argc > 3) ? argv[3] : null;
						var domain = (argc > 4) ? argv[4] : null;
						var secure = (argc > 5) ? argv[5] : false;
						var cookieVal = name + "=" + encodeURIComponent (value);
						cookieVal += "; expires=" + ((expires == null) ? "Sun, 31 Jan 2050 12:00:00 GMT" : expires);
						cookieVal += ((path == null) ? "" : ("; path=" + path));
						cookieVal += ((domain == null) ? "" : ("; domain=" + domain));
						cookieVal += ((secure == true) ? "; secure" : "");
						document.cookie = cookieVal;
					},
					getCookie : function (Name) { 
						var search = Name + "=";
					    if (document.cookie.length > 0) { 
					        var offset = document.cookie.indexOf(search) ;
					        if (offset != -1) { 
					            offset += search.length; 
					            var end = document.cookie.indexOf(";", offset);
					            if (end == -1) end = document.cookie.length 
					            return decodeURIComponent(document.cookie.substring(offset, end)) 
					        } 
					    } 
					    return "";
					},
					deleteCookie : function (name) {
						ClickLib.Functions.setCookie(name, "", "Thu, 01-Jan-1970 00:00:01 GMT");
					}
				});
				
				/*****  login button  *****/
				ClickLib.Page.bindControl({
					delegationParentSelector: "#login",
					elementId : "loginBtn",
					form : "login",
					submitForm : true,
					beforeSubmit : function () {
						ClickLib.Functions.setCookie("email", document.login.j_username.value);
						return true;
					}
				});
				
				/*****  forgot password button  *****/
				ClickLib.Page.bindControl({
					delegationParentSelector: "#login",
					elementId : "forgotPasswordLink",
					form : "pwRequest",
					submitForm : true,
					ignoreXref: true,
					beforeSubmit : function (element) {
						var email = jQuery(idSelector("j_username")).val();
					    if (!email || jQuery.trim(email) === ""){
							ClickLib.Page.addErrorMessages(["Please enter your email address."])
							return false;
						}
						jQuery(idSelector("forgotPasswordEmailField")).val(email);
						return true;
					}
				});
						
